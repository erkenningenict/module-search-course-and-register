{
  "name": "my",
  "request": {
    "url": "/graphql",
    "method": "POST",
    "body": "(.*)getMy\\(\\$input(.*)"
  },
  "responses": {
    "getUserNonGba": {
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/user-with-ug-kba-non-gba.json"
    },
    "getUserGba": {
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/user-with-ug-kba-gba.json"
    },
    "getNonStudent": {
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/user-without-student-role.json"
    },
    "getUnauthenticated": {
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/unauthenticated.json"
    }
  }
}
