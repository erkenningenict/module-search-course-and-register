{
  "name": "@erkenningen/module-search-course",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3031",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start:mock": "run-p -r mockserver start:app",
    "start:app": "npm run dev",
    "build:netlify": "cp index.netlify.html index.html && npm run build",
    "lint": "eslint src --max-warnings=0",
    "mockserver": "DEBUG=ng-apimock:* node ./mocks/mocks.mjs",
    "prettier:all": "prettier --write \"**/*.{css,js,json,md,scss,ts,tsx,html}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.9.7",
    "@erkenningen/config": "^2.0.0",
    "@erkenningen/ui": "^8.0.2",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "2.5.1",
    "date-fns": "3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "formik": "2.4.6",
    "graphql": "15.8.0",
    "primeicons": "6.0.1",
    "primereact": "10.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.23.1",
    "react-transition-group": "4.4.5",
    "ts-essentials": "^9.4.2",
    "typescript": "5.4.2",
    "use-query-params": "2.2.1",
    "vite": "^5.2.11",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@ng-apimock/core": "^3.11.2",
    "@ng-apimock/dev-interface": "^3.3.1",
    "@types/classnames": "2.3.1",
    "@types/node": "20.11.28",
    "express": "^4.19.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
