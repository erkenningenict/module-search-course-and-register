{
  "name": "@erkenningen/module-search-course",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:mock": "run-p -r mockserver start:app",
    "start:app": "react-scripts start --no-cache",
    "build": "react-scripts build",
    "build:netlify": "cp public/index.netlify.html public/index.html && npm run build",
    "test": "react-scripts test",
    "lint": "eslint src --max-warnings=0",
    "eject": "react-scripts eject",
    "mockserver": "node ./mocks/mocks.js",
    "prettier:all": "prettier --write \"**/*.{css,js,json,md,scss,ts,tsx,html}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@erkenningen/config": "1.0.7",
    "@erkenningen/ui": "7.0.1",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-apollo": "^3.1.6",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "classnames": "2.3.1",
    "date-fns": "2.27.0",
    "es6-shim": "0.35.6",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "formik": "2.2.9",
    "graphql": "15.4.0",
    "primeicons": "5.0.0",
    "primereact": "7.0.1",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-router-dom": "6.1.1",
    "react-scripts": "4.0.3",
    "react-transition-group": "4.4.2",
    "ts-essentials": "^9.0.0",
    "typescript": "4.4.2",
    "use-query-params": "1.2.3",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@erkenningen/build": "1.0.7",
    "@types/classnames": "2.3.1",
    "@types/jest": "24.0.18",
    "@types/node": "16.11.12",
    "@types/yup": "0.29.13",
    "ng-apimock": "1.4.9",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1"
  },
  "proxy": "http://localhost:4000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
